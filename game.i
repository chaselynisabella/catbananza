void updateGame() {
    updateMouse();


    catTimer++;

    if (catTimer % 100 == 0) {
        catAppear();
    }
    for (int i = 0; i < 10; i++) {
        updateCats(&cats[i]);
    }


    cheeseTimer++;
    if (cheeseTimer % 100 == 0) {
       cheeseAppear();
    }
    for (int i = 0; i < 10; i++) {
        updateCheese(&cheese[i]);
    }


    winCTimer++;
    if (winCTimer > 1000) {
        if (winCTimer % 100 == 0) {
            winCAppear();
        }
    }
    for (int i = 0; i < 3; i++) {
        updateWinC(&winCheese[i]);
    }


    pillarTimer++;
    if (pillarTimer % 100 == 0) {
        pillarAppear();
    }


    for (int i = 0; i < 10; i++) {
  updateBullets(&bullets[i]);
    }
    updateLives();


    hOff++;
    waitForVBlank();
    (*(volatile unsigned short *)0x04000010) = hOff;
}

void drawGame() {
    drawMouse();
    for (int i = 0; i < 10; i++) {
        drawCats(&cats[i]);
    }
    for (int i = 0; i < 10; i++) {
        drawCheese(&cheese[i]);
    }
    for (int i = 0; i < 3; i++) {
        drawWinC(&winCheese[i]);
    }
    for (int i = 0; i < 10; i++) {
  drawBullets(&bullets[i]);
    }
    for (int i = 0; i < 10; i++) {
        drawPillar(&pillars[i]);
    }
    drawLives();

    waitForVBlank();
    DMANow(3, shadowOAM, ((OBJ_ATTR*)(0x7000000)), 128 * 4);

    (*(volatile unsigned short *)0x04000010) = hOff;
    (*(volatile unsigned short *)0x04000012) = vOff;
}

void initMouse() {
    mouse.hOff = 0;
 mouse.worldRow = 60;
 mouse.worldCol = 20;
    mouse.screenRow = mouse.worldRow - vOff;
    mouse.screenCol = mouse.worldCol - mouse.hOff;
 mouse.height = 16;
 mouse.width = 16;
 mouse.rdel = 1;
 mouse.cdel = 1;
 mouse.aniState = SPRITEIDLE;
    mouse.tileCol = 1;
    mouse.tileRow = 1;
 mouse.curFrame = 0;
 mouse.active = 1;
 mouse.hide = 0;
 mouse.aniCounter = 0;
 mouse.numFrames = 4;
    mouse.cheatState = 0;
}


void drawMouse() {
    shadowOAM[0].attr0 = (0xFF & mouse.worldRow) | (0<<14);
 shadowOAM[0].attr1 = (0x1FF & mouse.worldCol) | (1<<14);
 shadowOAM[0].attr2 = ((0)*32+(mouse.aniState * 2));
}


void updateMouse() {

    if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<6)))) {
        if (mouse.worldRow > 0) {
            mouse.aniState = SPRITEBACK;
            mouse.worldRow--;
        }
    }
    if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<7)))) {
        if(mouse.worldRow + mouse.height < 160) {
            mouse.aniState = SPRITEFRONT;
            mouse.worldRow++;
        }
    }
    if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<4)))) {
        if(mouse.worldCol + mouse.width < 240) {
            mouse.aniState = SPRITERIGHT;
            mouse.worldCol++;
        }
    }
    if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<5)))) {
        if(mouse.worldCol> 0) {
            mouse.aniState = SPRITELEFT;
            mouse.worldCol -= 2;
        }
    }


    if (mouse.worldCol <= 0) {
        livesLeft--;
    }

    if (mouse.aniState != SPRITEIDLE) {
  mouse.prevAniState = mouse.aniState;
  mouse.aniState = SPRITEIDLE;
 }

    if(mouse.aniCounter % 10 == 0) {
  mouse.curFrame++;
  if (mouse.curFrame >= mouse.numFrames) {
   mouse.curFrame = 0;
  }
 }
    if (mouse.aniState == SPRITEIDLE) {
  mouse.curFrame = 0;
  mouse.aniState = mouse.prevAniState;
 } else {
  mouse.aniCounter++;
 }


    if ((!(~(oldButtons)&((1<<0))) && (~buttons & ((1<<0)))) && mouse.aniState == SPRITELEFT && ammoCount > 0) {
  playSoundB(shootSound_data,shootSound_length, 0);
  shootLeft();
        ammoCount--;
 }

 if ((!(~(oldButtons)&((1<<0))) && (~buttons & ((1<<0)))) && mouse.aniState == SPRITERIGHT && ammoCount > 0) {
  playSoundB(shootSound_data,shootSound_length, 0);
  shootRight();
        ammoCount--;
 }
 if ((!(~(oldButtons)&((1<<0))) && (~buttons & ((1<<0)))) && mouse.aniState == SPRITEBACK && ammoCount > 0) {
  playSoundB(shootSound_data,shootSound_length, 0);
  shootUp();
        ammoCount--;
 }
 if ((!(~(oldButtons)&((1<<0))) && (~buttons & ((1<<0)))) && mouse.aniState == SPRITEFRONT && ammoCount > 0) {
        playSoundB(shootSound_data,shootSound_length, 0);
  shootDown();
        ammoCount--;
 }
    if ((!(~(oldButtons)&((1<<2))) && (~buttons & ((1<<2))))) {
     mouse.cheatState = 1;
    }


    for (int i = 0; i < 10; i++) {
        if (mouse.cheatState == 0) {
            if (cats[i].active) {
          if (collision(mouse.worldCol, mouse.worldRow, mouse.width, mouse.height,
            cats[i].screenCol - hOff, cats[i].screenRow, cats[i].height, cats[i].width) == 1) {
       livesLeft--;
                cats[i].active = 0;
      }
            }
     } else {
            break;
        }
    }


    for (int i = 0; i < 10; i++) {
        if (cheese[i].active) {
            if (collision(mouse.worldCol, mouse.worldRow, mouse.width, mouse.height,
            cheese[i].screenCol - hOff, cheese[i].screenRow, cheese[i].height, cheese[i].width) == 1) {
                cheese[i].active = 0;
                ammoCount++;
      }
        }
    }


    for (int i = 0; i < 3; i++) {
        if (winCheese[i].active) {
            if (collision(mouse.worldCol, mouse.worldRow, mouse.width, mouse.height,
            winCheese[i].screenCol - hOff, winCheese[i].screenRow, winCheese[i].height, winCheese[i].width)) {
                hOff = 0;
                goToWin();
      }
        }
    }


    for (int i = 0; i < 10; i++) {
        if (pillars[i].active) {
      if (collision(mouse.worldCol, mouse.worldRow, mouse.width, mouse.height,
                pillars[i].screenCol - hOff, pillars[i].screenRow, pillars[i].height, pillars[i].width) == 1) {
       mouse.worldCol -= 20;
      }
        }
 }
}


