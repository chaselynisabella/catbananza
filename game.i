     if (!bullets[i].active) {

   bullets[i].direction = 3;


   bullets[i].worldRow = mouse.worldRow + 2;
   bullets[i].worldCol = mouse.worldCol + (mouse.width/2);

   bullets[i].hide = 0;
   bullets[i].active = 1;




