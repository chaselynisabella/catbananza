void catAppear() {
    for (int i = 0; i < 10; i++) {
  if (!cats[i].active) {

   cats[i].screenRow = rand() % 100 + 20;
            cats[i].screenCol = 240 + hOff;


            cats[i].cdel = rand() % 2 + 1;
            cats[i].hOff++;


   cats[i].active = 1;

   break;
  }
 }
}


void updateCats(ANISPRITE* c) {
    if (c->active) {
        if (c->worldRow < mouse.worldRow) {
            c->screenRow += c->rdel;
        } else if (c->worldRow >= mouse.worldRow) {
            c->screenRow -= c->rdel;
        }
        c->screenCol -= c->cdel;
        if (c->screenRow <= 1 || c->screenRow + c->height-1 >= 160)
   c->rdel *= -1;


        for (int i = 0; i < 10; i++) {
            if (bullets[i].active && collision(c->screenCol - hOff, c->screenRow, c->width, c->height,
                bullets[i].worldCol, bullets[i].worldRow, bullets[i].width, bullets[i].height)) {

                bullets[i].active = 1;
                c->active = 0;
            }
        }
    }
}


void drawCats() {
    for (int i = 0; i < 10; i++) {
        if (cats[i].active) {
            shadowOAM[4+i].attr0 = (0xFF & cats[i].screenRow) | (0<<14);
            shadowOAM[4+i].attr1 = (0x1FF & cats[i].screenCol - hOff) | (1<<14);
            shadowOAM[4+i].attr2 = ((4)*32+(0));
        } else {
            shadowOAM[4 + i].attr0 |= (2<<8);
        }
    }
}

