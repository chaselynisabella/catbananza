void updateBullets(ANISPRITE * b) {
    if (b->active && b->direction == 1
    && b->worldCol + b->cdel <= 240 -1
    && b->worldCol + b->cdel > 0 - b->width) {
   b->worldCol -= b->cdel + 2;

  } else if (b->worldRow + b->height-1 >= 0 && b->direction == 2 && b->worldCol + b->cdel > 0 && b->worldCol + b->cdel < 240 -1) {
   b->worldCol += b->cdel + 2;
  } else if (b->active && b->direction == 3
    && b->worldRow + b->rdel <= 160 -1
    && b->worldRow + b->rdel > 0 - b->height ) {

   b->worldRow -= b->rdel + 2;

  } else if (b->active && b->direction == 4
    && b->worldRow + b->rdel <= 160 + b->height
    && b->worldRow + b->rdel > 0 - b->height ) {

   b->worldRow += b->rdel + 2;

  } else {
   b->active = 0;

  }
}


